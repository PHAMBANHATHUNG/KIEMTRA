            </ListBox.RenderTransform>
        </ListBox>
        <Button x:Name="CAT" Content="con meo" HorizontalAlignment="Left" Margin="129,325,0,0" VerticalAlignment="Top" Width="75" Click="CAT_Click"/>
        <Button x:Name="DOG" Content="Con cho" HorizontalAlignment="Left" Margin="606,325,0,0" VerticalAlignment="Top" Width="75" Click="DOG_Click"/>

    </Grid>
</Window>
